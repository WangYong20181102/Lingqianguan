<?xml version="1.0" encoding="utf-8"?><!-- 提现成功 -->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/immersion_status"
    android:orientation="vertical">

    <!-- 标题 -->

    <include layout="@layout/include_activity_title" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/d20"
                    android:gravity="center_vertical">

                    <ImageView
                        android:layout_width="@dimen/d23"
                        android:layout_height="@dimen/d23"
                        android:layout_marginLeft="@dimen/d15"
                        android:src="@drawable/withdrawal_success" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d15"
                        android:text="提现申请成功"
                        android:textColor="@color/FC_333333"
                        android:textSize="@dimen/s15" />

                </LinearLayout>

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <View
                        android:id="@+id/v_withdrawal0"
                        style="@style/line_h3"
                        android:layout_marginLeft="25.5dp" />

                    <View
                        android:id="@+id/v_withdrawal1"
                        android:layout_width="@dimen/d10"
                        android:layout_height="@dimen/d10"
                        android:layout_below="@+id/v_withdrawal0"
                        android:layout_marginLeft="21.5dp"
                        android:background="@drawable/withdrawal_dot" />

                    <View
                        style="@style/line_h4"
                        android:layout_below="@+id/v_withdrawal1"
                        android:layout_marginLeft="25.5dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:layout_marginLeft="21.5dp"
                        android:layout_toRightOf="@+id/v_withdrawal1"
                        android:text="提现处理中"
                        android:textColor="@color/FC_333333"
                        android:textSize="@dimen/s15" />
                </RelativeLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_vertical">

                    <ImageView
                        android:layout_width="@dimen/d23"
                        android:layout_height="@dimen/d23"
                        android:layout_marginLeft="@dimen/d15"
                        android:src="@drawable/withdrawal_success_money" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d15"
                        android:text="到账后将会短信通知您"
                        android:textColor="@color/FC_333333"
                        android:textSize="@dimen/s15" />

                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/d15"
                    android:layout_marginLeft="@dimen/d53"
                    android:layout_marginTop="@dimen/d5"
                    android:text="款项将于次日转入到您的银行卡账户\n(节假日以实际到账时间为准)"
                    android:textColor="@color/FC_999999"
                    android:textSize="@dimen/s13" />

                <!--    <ImageView
                        android:id="@+id/imageView1"
                        android:layout_width="@dimen/d60"
                        android:layout_height="@dimen/d60"
                        android:layout_marginBottom="@dimen/d10"
                        android:layout_marginTop="@dimen/d20"
                        android:src="@drawable/withdrawal_success" />

                    <com.wtjr.lqg.widget.UpDownTextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:down_text_color="@color/FC_666666"
                        app:down_text_content="款项将于次日转入到您的银行卡账户\n(节假日以实际到账时间为准)"
                        app:down_text_size="@dimen/s13"
                        app:text_gravity="center"
                        app:up_down_margin="@dimen/d8"
                        app:up_text_color="#fa8f21"
                        app:up_text_content="恭喜你，提现申请成功！"
                        app:up_text_size="@dimen/s18" />

                     <TextView
                         android:layout_width="match_parent"
                         android:layout_height="@dimen/d3"
                         android:layout_marginBottom="@dimen/d5"
                         android:layout_marginTop="@dimen/d10"
                         android:background="@drawable/xuxian"
                         android:layerType="software" />
             -->
                <View style="@style/line" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d45"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d15"
                        android:layout_weight="1"
                        android:text="银行名称"
                        android:textColor="@color/FC_666666"
                        android:textSize="@dimen/s15" />

                    <ImageView
                        android:id="@+id/tv_bankCardImage"
                        android:layout_width="@dimen/d25"
                        android:layout_height="@dimen/d25"
                        android:src="@drawable/image_bank_moreng" />

                    <TextView
                        android:id="@+id/tv_bankCardName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d10"
                        android:layout_marginRight="@dimen/d15"
                        android:text="XXXX银行"
                        android:textColor="@color/FC_999999"
                        android:textSize="@dimen/s15" />
                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d45"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d15"
                        android:layout_weight="1"
                        android:text="银行卡号"
                        android:textColor="@color/FC_666666"
                        android:textSize="@dimen/s15" />

                    <TextView
                        android:id="@+id/tv_bank_num"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/d15"
                        android:textColor="@color/FC_999999"
                        android:textSize="@dimen/s15" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d45"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d15"
                        android:layout_weight="1"
                        android:text="姓名"
                        android:textColor="@color/FC_666666"
                        android:textSize="@dimen/s15" />

                    <TextView
                        android:id="@+id/tv_bank_usename"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/d15"
                        android:textColor="@color/FC_999999"
                        android:textSize="@dimen/s15" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d45"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d15"
                        android:layout_weight="1"
                        android:text="申请金额"
                        android:textColor="@color/FC_666666"
                        android:textSize="@dimen/s15" />

                    <TextView
                        android:id="@+id/tv_withdrawMoney"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/d15"
                        android:textColor="@color/FC_999999"
                        android:textSize="@dimen/s15" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d45"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d15"
                        android:layout_weight="1"
                        android:text="手续费"
                        android:textColor="@color/FC_666666"
                        android:textSize="@dimen/s15" />

                    <TextView
                        android:id="@+id/tv_withPoundage"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/d15"
                        android:text="2.00元"
                        android:textColor="@color/FC_999999"
                        android:textSize="@dimen/s15" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/d45"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/d15"
                        android:layout_weight="1"
                        android:text="到账金额"
                        android:textColor="@color/FC_666666"
                        android:textSize="@dimen/s15" />

                    <TextView
                        android:id="@+id/tv_withToAccount"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="@dimen/d15"
                        android:textColor="@color/FC_999999"
                        android:textSize="@dimen/s15" />
                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="@color/white"
                android:orientation="vertical">

                <Button
                    android:id="@+id/btn_next"
                    style="@style/buttonStyle_2_2"
                    android:layout_marginTop="@dimen/d15"
                    android:enabled="true"
                    android:text="完成"
                    android:textSize="@dimen/s15" />

            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
